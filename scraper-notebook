   "source": [
    "# categorize word count and make new column for category\n",
    "def categorize_word_count(word_count):\n",
    "    if word_count <= 30:\n",
    "        return \"0-30\"\n",
    "    elif word_count <= 60:\n",
    "        return \"31-60\"\n",
    "    elif word_count <= 90:\n",
    "        return \"61-90\"\n",
    "    elif word_count <= 120:\n",
    "        return \"91-120\"\n",
    "    elif word_count <= 150:\n",
    "        return \"121-150\"\n",
    "    elif word_count <= 180:\n",
    "        return \"151-180\"\n",
    "    else:\n",
    "        return \"180+\"\n",
    "\n",
    "master_data['word count category'] = master_data['post_word_count'].apply(categorize_word_count)\n",
    "master_data['count'] = 1\n",
    "master_data"
   ]
  },
