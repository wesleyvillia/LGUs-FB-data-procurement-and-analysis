   "source": [
    "#### Create scatter plot with data from all 3 citites from July 27 to August 6 "
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 24,
   "id": "ea1c149d",
   "metadata": {},
   "outputs": [],
   "source": [
    "# make new col if date it < 2021-07-24 (True or False)\n",
    "def check_25(date):\n",
    "    return not(int(date.replace(\"-\",\"\")) >= 20210725)\n",
    "\n",
    "qc_time_df['before 07-25'] = qc_time_df['date'].apply(check_25)\n",
    "mnl_time_df['before 07-25'] = mnl_time_df['date'].apply(check_25)\n",
    "cebu_time_df['before 07-25'] = cebu_time_df['date'].apply(check_25)\n",
    "\n",
    "# make copy dataframes for only False\n",
    "qc_after_07_25_df = qc_time_df.loc[qc_time_df.loc[:, 'before 07-25']== False,:].copy()\n",
    "mnl_after_07_25_df = mnl_time_df.loc[mnl_time_df.loc[:, 'before 07-25']== False,:].copy()\n",
    "cebu_after_07_25_df = cebu_time_df.loc[cebu_time_df.loc[:, 'before 07-25']== False,:].copy()"
   ]
  },
